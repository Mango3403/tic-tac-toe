{
  "name": "tictactoe-colyseus",
  "version": "1.0.2",
  "description": "HTML5 Tic-Tac-Toe Game powered by Colyseus",
  "scripts": {
    "install": "npm install --prefix ./server && npm install --prefix ./javascript-pixi",
    "start": "npm start --prefix ./javascript-pixi",
    "start:server": "npm start --prefix ./server",
    "start:client": "npm start --prefix ./javascript-pixi",
    "build:client": "npm run build --prefix ./javascript-pixi"
  },
  "author": "Endel Dreyer",
  "dependencies": {
    "css-loader": "^0.28.11",
    "json-loader": "^0.5.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "engines": {
    "node": "14.x"
  }
}
